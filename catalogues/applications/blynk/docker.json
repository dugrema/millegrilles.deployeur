{
  "nom": "blynk",
  "version": "1.34.2",
  "registries": [
    "docker.maceroc.com",
    "dugremat"
  ],
  "images": {
    "blynk": {
      "image": "mg_blynk",
      "version": "0.41.10_2"
    }
  },
  "backup": {
    "constraints": ["node.labels.millegrilles.app.blynk == true"],
    "data": {
      "volumes": ["blynk_data"]
    }
  },
  "dependances": [
    {
      "image": "blynk",
      "config": {
        "name": "blynk",
        "constraints": [
          "node.labels.millegrilles.app.blynk == true"
        ],
        "env": [
          "SERVER_SSL_KEY=/run/secrets/webkey.pem",
          "SERVER_SSL_CERT=/run/secrets/webcert.pem"
        ],
        "configs": [
          {
            "name": "pki.web.cert;pki.blynk.cert",
            "filename": "/run/secrets/webcert.pem"
          }
        ],
        "secrets": [
          {
            "name": "pki.web.key;pki.blynk.key",
            "filename": "webkey.pem"
          }
        ],
        "mounts": [
          "blynk_data:/blynk/data:rw"
        ],
        "endpoint_spec": {
          "mode": "vip",
          "ports": [
            {
              "published_port": 9443,
              "target_port": 9443,
              "protocol": "tcp",
              "publish_mode": "host"
            }
          ]
        },
        "networks": [
          {
            "target": "millegrille_net"
          }
        ],
        "labels": {
          "millegrille": "${IDMG}",
          "url": "https://${HOSTNAME}:9443",
          "securite": "2.prive"
        },
        "resources": {
          "cpu_limit": 1000000000,
          "mem_limit": 100000000
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    }
  ]
}