{
  "nom": "onionize",
  "version": "2022.2.2",
  "registries": [
    "torservers"
  ],
  "images": {
    "onionize": {
      "image": "onionize",
      "version": "release-0.3"
    }
  },
  "dependances": [
    {
      "image": "onionize",
      "config": {
        "name": "onionize",
        "mounts": [
          "/var/run/docker.sock:/tmp/docker.sock:ro",
          "onionize-config:/var/lib/tor:rw"
        ],
        "networks": [
          {
            "target": "millegrille_net"
          }
        ],
        "labels": {
          "application": "onionize",
          "millegrille": "${IDMG}"
        },
        "resources": {
          "cpu_limit": 500000000,
          "mem_limit": 73741824
        },
        "restart_policy": {
          "condition": "on-failure",
          "delay": 60000000000,
          "max_attempts": 10
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    }
  ]
}