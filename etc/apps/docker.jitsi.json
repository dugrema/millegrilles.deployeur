{
  "nom": "jitsi",
  "registries": [
    "jitsi"
  ],
  "images": {
    "jitsi_jvb": {
      "image": "jvb",
      "version": "stable-4627-1"
    },
    "jitsi_jicofo": {
      "image": "jicofo",
      "version": "stable-4627-1"
    },
    "jitsi_prosody": {
      "image": "prosody",
      "version": "stable-4627-1"
    },
    "jitsi_web": {
      "image": "web",
      "version": "stable-4627-1"
    }
  },
  "dependances": [
    {
      "image": "jitsi_jicofo",
      "generer": {
        "motsdepasse": [
          {"name": "passwd.jitsi.jicofoauth"},
          {"name": "passwd.jitsi.jicofosecret"},
          {"name": "passwd.jitsi.jvbauth"}
        ]
      },
      "config": {
        "name": "jitsi_jicofo",
        "constraints": [
          "node.labels.millegrilles.app.jitsi == true"
        ],
        "env": [
          "JICOFO_AUTH_USER=focus",
          "JVB_BREWERY_MUC=jvbbrewery",
          "TZ=America/Toronto",
          "XMPP_AUTH_DOMAIN=auth.meet.jitsi",
          "XMPP_DOMAIN=meet.jitsi",
          "XMPP_INTERNAL_MUC_DOMAIN=internal-muc.meet.jitsi",
          "XMPP_SERVER=jitsi_prosody",
          "JICOFO_AUTH_PASSWORD=c6891cd6b9d5986d1c876671498291dc",
          "JICOFO_COMPONENT_SECRET=794ae5de5ee01743b59871955b92c43b"
        ],
        "networks": [{
          "target": "mg_${IDMG}_net"
        }],
        "labels": {
          "millegrille": "${IDMG}"
        },
        "resources": {
          "cpu_limit": 320000000,
          "mem_limit": 128000000
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    },
    {
      "image": "jitsi_jvb",
      "config": {
        "name": "jitsi_jvb",
        "constraints": [
          "node.labels.millegrilles.app.jitsi == true"
        ],
        "env": [
          "JVB_TCP_HARVESTER_DISABLED=true",
          "DOCKER_HOST_ADDRESS=${HOSTNAME}",
          "JVB_AUTH_PASSWORD=d0869387093876bb3ed2c1a3ac668556",
          "JVB_AUTH_USER=jvb",
          "JVB_BREWERY_MUC=jvbbrewery",
          "JVB_PORT=10000",
          "JVB_STUN_SERVERS=meet-jit-si-turnrelay.jitsi.net:443",
          "TZ=America/Toronto",
          "XMPP_AUTH_DOMAIN=auth.meet.jitsi",
          "XMPP_INTERNAL_MUC_DOMAIN=internal-muc.meet.jitsi",
          "XMPP_SERVER=jitsi_prosody"
        ],
        "endpoint_spec": {
          "mode": "vip",
          "ports": [
            {
              "published_port": 10000,
              "target_port": 10000,
              "protocol": "udp",
              "publish_mode": "host"
            }
          ]
        },
        "networks": [
          {
            "target": "mg_${IDMG}_net"
          },
          {
            "target": "bridge"
          }
        ],
        "labels": {
          "millegrille": "${IDMG}"
        },
        "resources": {
          "cpu_limit": 2000000000,
          "mem_limit": 512000000
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    },{
      "image": "jitsi_prosody",
      "config": {
        "name": "jitsi_prosody",
        "constraints": [
          "node.labels.millegrilles.app.jitsi == true"
        ],
        "env": [
          "JICOFO_AUTH_PASSWORD=c6891cd6b9d5986d1c876671498291dc",
          "JICOFO_AUTH_USER=focus",
          "JICOFO_COMPONENT_SECRET=794ae5de5ee01743b59871955b92c43b",
          "JVB_AUTH_PASSWORD=d0869387093876bb3ed2c1a3ac668556",
          "JVB_AUTH_USER=jvb",
          "TZ=America/Toronto",
          "XMPP_AUTH_DOMAIN=auth.meet.jitsi",
          "XMPP_DOMAIN=meet.jitsi",
          "XMPP_GUEST_DOMAIN=guest.meet.jitsi",
          "XMPP_INTERNAL_MUC_DOMAIN=internal-muc.meet.jitsi",
          "XMPP_MUC_DOMAIN=muc.meet.jitsi",
          "XMPP_RECORDER_DOMAIN=recorder.meet.jitsi"
        ],
        "networks": [{
          "target": "mg_${IDMG}_net",
          "aliases": ["jitsi_prosody"]
        }],
        "labels": {
          "millegrille": "${IDMG}"
        },
        "resources": {
          "cpu_limit": 320000000,
          "mem_limit": 50000000
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    },
    {
      "image": "jitsi_web",
      "config": {
        "name": "jitsi_web",
        "constraints": [
          "node.labels.millegrilles.app.jitsi == true"
        ],
        "env": [
          "JICOFO_AUTH_USER=focus",
          "TZ=America/Toronto",
          "XMPP_AUTH_DOMAIN=auth.meet.jitsi",
          "XMPP_BOSH_URL_BASE=http://jitsi_prosody:5280",
          "XMPP_DOMAIN=meet.jitsi",
          "XMPP_GUEST_DOMAIN=guest.meet.jitsi",
          "XMPP_MUC_DOMAIN=muc.meet.jitsi",
          "XMPP_RECORDER_DOMAIN=recorder.meet.jitsi"
        ],
        "networks": [{
          "target": "mg_${IDMG}_net",
          "aliases": ["jitsi_web"]
        }],
        "labels": {
          "millegrille": "${IDMG}"
        },
        "resources": {
          "cpu_limit": 32000000,
          "mem_limit": 24000000
        },
        "mode": {
          "mode": "replicated",
          "replicas": 1
        }
      }
    }
  ],
  "nginx": {
    "server_file": "nginx.jitsi.conf",
    "subdomain": "jitsi",
    "params": {
      "PROXY_PASS_BACKEND": "https://jitsi_web:443"
    }
  }
}
