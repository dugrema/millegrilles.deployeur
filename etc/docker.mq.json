{
  "name": "mq",
  "env": [
    "MG_KEY=/run/secrets/key.pem",
    "WEB_KEY=/run/secrets/webkey.pem",
    "CONFIG_FILE=/etc/rabbitmq/rabbitmq_ipv6management.config"
  ],
  "mounts": [
    "mg-middleware-rabbitmq-data-${IDMG}:/var/lib/rabbitmq:rw",
    "mq-accounts:/opt/rabbitmq/conf:rw"
  ],
  "constraints": [
    "node.labels.millegrilles.mq == true"
  ],
  "configs": [
    {
      "name": "pki.mq.cert",
      "filename": "/opt/rabbitmq/dist/certs/cert.pem"
    },
    {
      "name": "pki.web.cert;pki.mq.cert",
      "filename": "/opt/rabbitmq/dist/certs/webcert.pem"
    }
  ],
  "secrets": [
    {
      "name": "pki.mq.key",
      "filename": "key.pem"
    },
    {
      "name": "pki.web.key;pki.mq.key",
      "filename": "webkey.pem"
    }
  ],
  "endpoint_spec": {
    "mode": "vip",
    "ports": [
      {
        "published_port": 8443,
        "target_port": 8443,
        "protocol": "tcp",
        "publish_mode": "host"
      },
      {
        "published_port": 5673,
        "target_port": 5673,
        "protocol": "tcp",
        "publish_mode": "host"
      }
    ]
  },
  "networks": [{
    "target": "mg_${IDMG}_net",
    "aliases": ["mq"]
  }],
  "labels": {
    "millegrille": "${IDMG}"
  },
  "resources": {
    "cpu_limit": 1000000000,
    "mem_limit": 629145600
  },
  "restart_policy": {
    "condition": "any",
    "delay": 30000000000
  },
  "mode": {
    "mode": "replicated",
    "replicas": 1
  }
}
